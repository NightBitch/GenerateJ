<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.SettingEntity" table="setting" schema="main">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="ftlPath">
            <column name="ftl_path" sql-type="text" not-null="true"/>
        </property>
        <property name="outPath">
            <column name="out_path" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>